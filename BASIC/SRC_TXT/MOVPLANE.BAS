1 SCREEN 1
2 COLOR 1, 1
10 X = 0
11 Y = 20
12 XB = 0
14 YB = 21
20 CLS
21 LINE (X, Y)-(X + 5, Y)
22 LINE (X + 4, Y + 2)-(X + 4, Y - 2)
23 CIRCLE (XB, YB), 1
24 LINE (0, 140)-(300, 140)
25 LOCATE 19, 1
26 PRINT "PRESS A TO GO LEFT"; " PRESS D TO GO RIGHT"
27 PRINT "PRESS S TO GO DOWN"; " PRESS W TO GO UP"
28 PRINT "PRESS 0 TO EXIT"
29 PRINT "X:"; X; " Y:"; Y; " XB:"; XB; " YB:"; YB
34 V$ = INPUT$(1)
35 IF V$ = "0" GOTO 60
36 IF V$ = "A" GOTO 40
37 IF V$ = "D" GOTO 45
38 IF V$ = "S" GOTO 50
39 IF V$ = "W" GOTO 55
40 X = X - 1
41 XB = XB - 1
42 GOTO 20
45 X = X + 1
46 XB = XB + 1
47 GOTO 20
50 Y = Y + 1
51 YB = YB + 1
52 GOTO 20
55 Y = Y - 1
56 YB = YB - 1
57 GOTO 20
60 CLS
62 PRINT " END DEMO OF MOVING PLANE"
63 PRINT " BY GGITKO CO. INC.(C)(R)"
64 PRINT " GGITKO, GGITKO CO. INC.,"
65 PRINT " MOVPLANE ARE REG. TM-s  "
66 PRINT " #STAYHOME"



